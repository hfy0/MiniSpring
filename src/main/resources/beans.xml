<?xml version="1.0" encoding="UTF-8"?>
<beans>
    <bean id="baseBaseService" class="org.example.test.BaseBaseService">
        <property type="org.example.test.AServiceImpl" name="as" ref="aService"/>
    </bean>

    <bean id="aService" class="org.example.test.AServiceImpl">
        <constructor-arg type="String" name="name" value="abc"/>
        <constructor-arg type="int" name="level" value="3"/>
        <property type="String" name="property1" value="Someone says"/>
        <property type="String" name="property2" value="Hello World!"/>
        <property type="org.example.test.BaseService" name="ref1" ref="baseService"/>
    </bean>

    <bean id="baseService" class="org.example.test.BaseService">
<!--        <property type="org.example.test.BaseBaseService" name="bbs" ref="baseBaseService"/>-->
    </bean>

</beans>